# Docker Environment Variables
# Copy this file to .env and update with your actual values

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_minimum_32_characters_long

# AI API Keys
VOLTAGENT_API_KEY=your_voltagent_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Internal API Security
RAG_INTERNAL_API_KEY=your_internal_rag_api_key_here

# Database Passwords
MONGODB_ROOT_PASSWORD=your_mongodb_password_here

# ChromaDB Authentication (if enabled)
CHROMA_SERVER_AUTH_CREDENTIALS=admin:password